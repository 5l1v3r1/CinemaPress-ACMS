                                                <div class="button-settings">
                                                    <i class="fa fa-code"></i>
                                                    <p>
                                                        Коды
                                                    </p>
                                                </div>
                                            </a>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                            <a href="/<%- config.urls.admin %>/load">
                                                <div class="button-settings">
                                                    <i class="fa fa-bar-chart-o"></i>
                                                    <p>
                                                        Нагрузка
                                                    </p>
                                                </div>
                                            </a>
                                        </div>
